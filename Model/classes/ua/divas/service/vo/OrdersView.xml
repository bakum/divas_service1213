  <SQLQuery><![CDATA[SELECT Orders.ID, 
       Orders.DAT, 
       Orders.NUM, 
       Orders.KONTRAG_ID,
       (select fullname from kontragents where id = Orders.KONTRAG_ID) Kontrag_Name,
       Orders.CURR_ID, 
       Orders.KASSA_ID, 
       Orders.DIVISION_ID,
       (select fullname from divisions where id = division_id) Division_Name,
       Orders.USER_ID,
       (select login from users where id = user_id) User_name,
       Orders.KURS, 
       Orders.KRATNOST, 
       Orders.DISCRIPTION, 
       Orders.DELETED, 
       Orders.VERSION, 
       Orders.POSTED, 
       Orders.FIRM_ID, 
       Orders.ACTIVITIES_ID, 
       Orders.STATUS_ID,
       (select fullname from order_status where id = Orders.STATUS_ID) Status_Name,
       Orders.ZAMER_ID,
       (select fullname from kontragents where id = Orders.ZAMER_ID) Zamer_Name,
       (select login from users where id = (select user_id 
            from user_settings 
            where user_settings.zamerkontrag_id = Orders.ZAMER_ID)) Zamer_User_Name,
       (select user_id 
            from user_settings 
            where user_settings.zamerkontrag_id = Orders.ZAMER_ID) Zamer_User_Id,     
       Orders.DAT_ZAM, 
       Orders.TIME_ZAM, 
       Orders.SUMM_PLAN, 
       Orders.DAT_COMPLETE, 
       Orders.AGENT_ID, 
       Orders.DAT_COMPLETE_FACT, 
       Orders.DIVISION_DEST
FROM  ORDERS Orders
where posted = 1 and deleted = 0 and
status_id in (select id from order_status
where name in ('ВРасчете','Замер','Новый','НеОплачен','Контроль'))]]></SQLQuery>
